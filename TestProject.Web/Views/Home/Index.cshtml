<!DOCTYPE html>
<html ng-app="myModule">
<head>
    <title></title>

    @Styles.Render(
    "~/Content/bootstrap")
    
</head>
<body>
<div class="container " role="main" ng-controller="myController">
    <div class="jumbotron">Welcome to AngularJs</div>

    <div>
        <input type="text" ng-model="employeeFilter"/>
    </div>
    

    <ul>
        <li ng-repeat="employee in employees | filter: employeeFilter">{{ employee.FirstName + ' ' + employee.LastName }}</li>
    </ul>
</div>
        @Scripts.Render("~/bundles/angular")
        <script src="~/Scripts/app.js"></script>
</body>
</html>

